{
  "name": "frontend-checkout",
  "version": "2.12.6-alpha-3",
  "description": "Frontend checkout SDK",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "browser": "dist/index.module.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "prebuild": "yarn clean",
    "build": "microbundle src/**.ts --format=cjs,es --sourcemap=false --compress=true --jsx=React.createElement --external=react",
    "prebuild-watch": "yarn clean",
    "build-watch": "microbundle watch src/**.ts --format=cjs,es --no-compress --jsx=React.createElement --external=react",
    "clean": "rm -rf dist .rts2_*",
    "precommit": "lint-staged",
    "prepush": "tsc --noEmit --composite false && yarn test",
    "prettier": "prettier --check --ignore-path=../../.prettierignore \"src/**/*.{js,jsx,ts,tsx,json,css,html}\"",
    "prettier-fix": "prettier --write --ignore-path=../../.prettierignore \"src/**/*.{js,jsx,ts,tsx,json,css,html}\"",
    "lint": "eslint --ignore-pattern \"**/*.test.*\" --config ../../.eslintrc --ext .js,.jsx,.ts,.tsx src",
    "typescript": "tsc",
    "test": "jest --silent",
    "test-watch": "jest --watch",
    "test-coverage": "jest --coverage",
    "prepare": "yarn build"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn run prettier",
      "yarn run lint"
    ]
  },
  "devDependencies": {
    "jest-fetch-mock": "^3.0.3",
    "microbundle": "^0.13.0",
    "tslib": "^2.2.0"
  },
  "peerDependencies": {
    "react": ">=17"
  },
  "sideEffects": false
}
